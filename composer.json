{
    "name": "react/async",
    "description": "Async utilities for React.",
    "keywords": ["async"],
    "license": "MIT",
    "require": {
        "php": ">=5.3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^4.8.35",
        "react/event-loop": "0.2.*"
    },
    "suggest": {
        "react/event-loop": "You need some kind of event loop for this to make sense."
    },
    "autoload": {
        "psr-0": { "React\\Async": "src" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
